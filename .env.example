SMARTY_WEBSITE_KEY=

# Paddle Payment Integration
# ============================================================
# SANDBOX (Testing): Get credentials from https://sandbox-vendors.paddle.com
# PRODUCTION (Live): Get credentials from https://vendors.paddle.com
# ============================================================
# Set to true for sandbox/testing, false for production/live
PADDLE_SANDBOX=true

# Your Paddle Vendor/Seller ID (found in Developer Tools > Authentication)
PADDLE_VENDOR_ID=

# Your Paddle API Key (generate in Developer Tools > Authentication)
PADDLE_API_KEY=

# Webhook Secret (found in Developer Tools > Notifications after creating webhook)
PADDLE_WEBHOOK_SECRET=

# Client-side Token for Paddle.js (generate in Developer Tools > Authentication)
# Sandbox tokens start with "test_", Live tokens start with "live_"
PADDLE_CLIENT_TOKEN=
